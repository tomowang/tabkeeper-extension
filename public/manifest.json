{
  "manifest_version": 3,
  "name": "TabKeeper",
  "version": "1.0",
  "description": "TabKeeper manages tabs and tab groups for your browser",
  "permissions": ["storage", "tabs", "tabGroups"],
  "author": "xiaoliang.zero@gmail.com",
  "icons": {
    "16": "images/logo-16.png",
    "32": "images/logo-32.png",
    "128": "images/logo-128.png"
  },
  "background": {
    "service_worker": "public/background.js"
  },
  "action": {
    "default_popup": "index.html"
  }
}
